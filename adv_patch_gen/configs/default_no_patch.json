{
    "image_dir": "../datasets/FLIR/images/train",
    "label_dir": "../datasets/FLIR/labels/train",
    "val_image_dir": "../datasets/FLIR/images/val",
    "use_even_odd_images": "all",
    "log_dir": "./runs/train_adversarial/placeholder",
